@using FinalProject.Models
@{
     Admins admin = (Admins)Session["Loginner"];
}
<!DOCTYPE html>
<html lang="en">


<!-- Mirrored from coderthemes.com/zircos/layouts/vertical/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Tue, 11 Aug 2020 13:14:38 GMT -->
<head>
    <meta charset="utf-8" />
    <title>Dashboard 1 | Zircos - Responsive Bootstrap 4 Admin Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Responsive bootstrap 4 admin template" name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Areas/Admin/Public/images/favicon.ico">

    <!-- App css -->
    <link href="~/Areas/Admin/Public/css/bootstrap.min.css" rel="stylesheet" type="text/css" id="bootstrap-stylesheet" />
    <link href="~/Areas/Admin/Public/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Public/css/app.min.css" rel="stylesheet" type="text/css" id="app-stylesheet" />
    <link href="~/Areas/Admin/Public/css/style.css" rel="stylesheet" type="text/css" id="app-stylesheet" />
    @RenderSection("dtpCss", required: false)

</head>

<body>

    <!-- Begin page -->
    <div id="wrapper">
        <!-- Topbar Start -->
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-right mb-0">
                <li class="dropdown notification-list">
                    <a class="nav-link dropdown-toggle nav-user mr-0 waves-effect" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <img src="~/Uploads/Admins/@(admin.Image!=null?admin.Image:"profile_img.png")" alt="user-image" class="rounded-circle">
                        <span class="d-none d-sm-inline-block ml-1">@admin.Name @admin.Surname.Remove(1).</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                        <!-- item-->
                        <div class="dropdown-header noti-title">
                            <h6 class="text-overflow m-0">Welcome !</h6>
                        </div>

                        <!-- item-->
                        <a href="@Url.Action("Logout","Home")" class="dropdown-item notify-item">
                            <i class="mdi mdi-logout-variant"></i>
                            <span>Logout</span>
                        </a>

                    </div>
                </li>
            </ul>

            <!-- LOGO -->
            <div class="logo-box">
                <a href="@Url.Action("Index","Home")" class="logo text-center">
                    <span class="logo-lg">
                        <img src="~/Areas/Admin/Public/images/logo-light.png" alt="" height="18">
                        <!-- <span class="logo-lg-text-light">Zircos</span> -->
                    </span>
                    <span class="logo-sm">
                        <!-- <span class="logo-sm-text-dark">Z</span> -->
                        <img src="~/Areas/Admin/Public/images/logo-sm.png" alt="" height="24">
                    </span>
                </a>
            </div>

            <ul class="list-unstyled topnav-menu topnav-menu-left m-0">
                <li>
                    <button class="button-menu-mobile waves-effect">
                        <i class="mdi mdi-menu"></i>
                    </button>
                </li>
            </ul>
        </div>
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start (Navigation)========== -->
        <div class="left-side-menu">
            <div class="slimscroll-menu">
                <!--- Sidemenu -->
                <div id="sidebar-menu">
                    <ul class="metismenu" id="side-menu">
                        <li class="menu-title">Navigation</li>
                        <li>
                            <a href="@Url.Action("Index","Home")" class="waves-effect waves-light">
                                <i class="mdi mdi-view-dashboard"></i>
                                <span>  Dashboard  </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Admin")" class="waves-effect waves-light">
                                <i class="mdi mdi-invert-colors"></i>
                                <span> Admin </span>
                            </a>
                        </li>
                        <li>
                            <a href="javascript: void(0);" class="waves-effect waves-light">
                                <i class="mdi mdi-layers"></i>
                                <span>  Pages </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="@Url.Action("Index","Faq")">FAQ</a></li>
                                <li><a href="@Url.Action("Index","Setting")">Page Setting</a></li>
                                <li><a href="@Url.Action("Index","Contact")">Contact</a></li>
                                <li><a href="@Url.Action("Index","SocialNets")">Social Networks</a></li>
                                <li><a href="@Url.Action("Index","Partners")">Partners</a></li>
                                <li><a href="@Url.Action("Index","Testimonial")">Testimonial</a></li>
                                <li><a href="@Url.Action("Index","Terms")">Terms And Conditions</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","SaleBanner")" class="waves-effect waves-light">
                                <i class="mdi mdi-diamond-stone"></i>
                                <span>  Sale Banner  </span>
                            </a>
                        </li>
                        <li>
                            <a href="javascript: void(0);" class="waves-effect waves-light">
                                <i class="mdi mdi-diamond-stone"></i>
                                <span>  Products  </span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="@Url.Action("Index","Products")">Product</a></li>
                                <li><a href="@Url.Action("Index","ProductInfo")">Product Details</a></li>
                                <li><a href="@Url.Action("Index","Colors")">Colors</a></li>
                                <li><a href="@Url.Action("Index","Sizes")">Size</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="javascript: void(0);" class="waves-effect waves-light">
                                <i class="mdi mdi-diamond-stone"></i>
                                <span>  Product Categories  </span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="@Url.Action("Index","Category")">Category</a></li>
                                <li><a href="@Url.Action("Index","Subcategory")">Sabcategory</a></li>
                                <li><a href="@Url.Action("Index","SubOfSubcategories")">Sub of Subcategory</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="javascript: void(0);" class="waves-effect waves-light">
                                <i class="mdi mdi-comment-text-outline"></i>
                                <span>  Blog  </span>
                                <span class="menu-arrow"></span>
                            </a>
                            <ul class="nav-second-level" aria-expanded="false">
                                <li><a href="@Url.Action("Index","Blogs")">Blogs</a></li>
                                <li><a href="@Url.Action("Index","BlogCategories")">Category</a></li>
                                <li><a href="@Url.Action("Index","BlogTags")">Tag</a></li>

                            </ul>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Messages")" class="waves-effect waves-light">
                                <i class="mdi mdi-email"></i>
                                <span>  Messages </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Users")" class="waves-effect waves-light">
                                <i class="mdi mdi-diamond-stone"></i>
                                <span>  Users  </span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Subscribes")" class="waves-effect waves-light">
                                <i class="mdi mdi-home-map-marker"></i>
                                <span>  Subscribe </span>
                                <span class="menu-arrow"></span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- End Sidebar -->

            </div>
            <!-- Sidebar -left -->

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <div class="content">
                <!-- Start Content-->
                <div class="container-fluid">
                    @RenderBody()
                </div>
                <!-- end container-fluid -->
            </div>
            <!-- end content -->
            <!-- Footer Start -->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            2018 - 2020 &copy; Zircos theme by <a href="#">Coderthemes</a>
                        </div>
                    </div>
                </div>
            </footer>
            <!-- end Footer -->
        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->

    </div>
    <!-- END wrapper -->
    <!-- Vendor js -->
    <script src="~/Areas/Admin/Public/js/vendor.min.js"></script>

    <script src="~/Areas/Admin/Public/libs/morris-js/morris.min.js"></script>
    <script src="~/Areas/Admin/Public/libs/raphael/raphael.min.js"></script>

    <script src="~/Areas/Admin/Public/js/pages/dashboard.init.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    @RenderSection("dtp", required: false)
    @RenderSection("ckEditor", required: false)

    <!-- App js -->
    <script src="~/Areas/Admin/Public/js/app.min.js"></script>
    <script src="~/Areas/Admin/Public/js/script.js"></script>
    <script src="~/Areas/Admin/Public/js/mainScript.js"></script>

</body>


